}
.list ::-webkit-scrollbar {
	width: 10px;
	height: 10px;
}
.list ::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    -webkit-border-radius: 10px;
    border-radius: 10px;
}
